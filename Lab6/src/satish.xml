<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	<bean id="aobj" class="com.satishlabs.A">
		<property name="a" value="10"/>
		<property name="msg" value="Satish"/>
	</bean>
	<bean id="aobj1" class="com.satishlabs.A">
		<property name="a" value="30"/>
		<property name="msg" value="Hello Satish"/>
	</bean>
	<bean id="bobj" class="com.satishlabs.B">
		<constructor-arg value="20"/>
		<constructor-arg value="Prasad"/>
	</bean>
	<bean id="hello" class="com.satishlabs.Hello" autowire="byType"></bean>
</beans>